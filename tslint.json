{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "interface-name": [false],
    "no-console": [true, { "allow": ["warn", "error"] }],
    "object-literal-sort-keys": false,
    "prettier": true
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}
